    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vscode-loc.git": "git+https://github.com/microsoft/vscode-loc.git"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.x || ^1.0.1-beta.0"
